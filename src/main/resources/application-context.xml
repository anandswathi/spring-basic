<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean name="fileMessageReader"
		class="com.yaliprowess.spring.session.basic.message.impl.FileMessageReaderImpl">
		<constructor-arg value="res/file.txt" />
	</bean>
	
	<!-- Abstract -->
	<bean name="readerService" class="com.yaliprowess.spring.session.basic.service.ReaderService">
		<!-- <constructor-arg ref="fileMessageReader" /> -->
		<constructor-arg ref="fileMessageReader" />
	</bean>
	
</beans>